{

"default": {
    "services": [
        "learning-switch",
        "link-discovery",
        "host-manager",
        "firewall",
        "stp",
        "maple"
    ],

    "tables": {
        "maple" : 0
    },


    "controller": {
         "nthreads": 1,
         "port": 6652,
         "cbench": false
   },

    "maple": {
          "pipeline": [
             "link-discovery",
             "firewall",
             "host-manager",
             "forwarding"
         ]
    },

    "loader": {
        "threads": 2
    },

    "link-discovery": {
        "poll-interval": 10,
        "pin-to-thread": 1
    },

    "switch-manager": {
        "pin-to-thread": 1
    },

    "switch-stats": {
	"poll-interval": 1,
	"pin-to-thread": 1
    }
}

}
