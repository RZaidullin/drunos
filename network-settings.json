{

"default": {
    "services": [
        "link-discovery",
        "rest-listener",
        "webui",
        "static-flow-pusher",
        "switch-stats",
        "stp",
        "stp-cli",
        "flow-manager",
        "rest-multipart",
        "command-line-interface",
        "rest-flowmod",
        "switch-manager-cli",
        "test-apps",
        "retic",
        "retic-cli"
    ],

    "retic":  {
        "main": "test-policy",
        "pin-to-thread": 1
    },

    "tables": {
        "static-flow-pusher" : 1,
        "retic": 2
    },

    "flow-manager" : {
        "interval" : 5
    },

    "rest-listener" : {
         "port" : 8000,
         "web-dir" : "./build/web"
    },

    "controller": {
        "port": 6653,
         "nthreads": 1,
         "cbench": false
   },

    "loader": {
        "threads": 2
    },

    "link-discovery": {
        "poll-interval": 10,
        "pin-to-thread": 1
    },

    "switch-manager": {
        "pin-to-thread": 1
    },

    "switch-stats": {
    "poll-interval": 1,
    "pin-to-thread": 1
    }
}

}
